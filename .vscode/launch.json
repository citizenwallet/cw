  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    },
    {
      "name": "Debug Attach",
      "type": "go",
      "request": "attach",
      "mode": "local"
    },
    {
      "name": "Launch Station",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/station/main.go",
      "args": ["-env", "-chainUrl", "http://localhost:8545", "-chainId", "1"]
    }
  ]
}
